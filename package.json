{
  "name": "temctl",
  "version": "1.0.0",
  "description": "跨平台AI终端工具 - 集成SSH管理、文件传输、AI交互和安全管控功能",
  "main": "src/main/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd src/backend && npm run dev",
    "dev:frontend": "cd src/frontend && npm run dev",
    "start": "electron src/main/main.js",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd src/backend && npm run build",
    "build:frontend": "cd src/frontend && npm run build",
    "pack": "electron-builder",
    "pack:all": "electron-builder --mac --win --linux",
    "test": "jest",
    "test:unit": "jest src/**/*.test.js",
    "lint": "eslint src --ext .js,.vue"
  },
  "keywords": [
    "ssh",
    "terminal",
    "ai",
    "ftp",
    "cross-platform",
    "electron",
    "security"
  ],
  "author": "ZhiPenTu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhiPenTu/Temctl.git"
  },
  "devDependencies": {
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "eslint": "^8.52.0",
    "eslint-plugin-vue": "^9.17.0"
  },
  "dependencies": {
    "ssh2": "^1.15.0",
    "sqlite3": "^5.1.6",
    "crypto-js": "^4.2.0",
    "winston": "^3.11.0",
    "axios": "^1.6.0"
  },
  "build": {
    "appId": "com.zhipentu.temctl",
    "productName": "Temctl",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/main/**/*",
      "src/backend/dist/**/*",
      "src/frontend/dist/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        {
          "target": "dmg",
          "arch": ["x64", "arm64"]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64", "ia32"]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": ["x64"]
        }
      ]
    }
  }
}